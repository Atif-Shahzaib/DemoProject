<% @products.each do |pro| %>
<div class="col-md-4">
    <div class="product">
        <h2 class="product-title"><%= pro.title %></h2>
        <p><%= pro.price %></p>
        <%= link_to "Details", product_path(pro), class: "btn-custom" %>
    </div>
</div>
<%end%>
<footer style="padding-bottom: 20px;">
<%= will_paginate @products, renderer: BootstrapPagination::Rails, previous_label: "Older", next_label: "Newer", inner_window: 1, outer_window: 0 %>
</footer>